	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>

	<title>Document</title>
</head>

<body>
	<div class="container">
		<p class="h1">Star Wars Wiki</p>
		<p class="h3">What would you like to know more about?</p>
		<div class="row">
			<button class="d-inline btn btn-primary" id="people">People</button>
			<button class="d-inline btn btn-primary" id="planets">Planets</button>
		</div>
	</div>


	<script>
		$(document).ready(function () {
			$('#people').click(function () {
				// let's make the request to our OWN server!
				$.get('/people', function (data) {
					// log the data to be sure we have it before we dive into manipulating the DOM
					console.log("got the data", data);
				}, 'json');
			});
		});
	</script>

	<body>
	</body>

